<ul class="collection-header with">
  <li class="collection-header center"><h5>search for friends</h5></li></ul>

<div class='container'>
  <%= form_tag(users_search_path, :method => "get") do %>
  <div class='input-field'>
    <%= text_field_tag :search, params[:search], placeholder: "find by phone number" %>
  </div>
  <div class='waves-effect waves-light btn light-blue darken-3'>
    <%= submit_tag "find friend" %>
  </div>
  <% end %>
</div>

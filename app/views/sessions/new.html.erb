<br><h4 class='container center-align'>login</h4><br>
<%= render '/errors' %>
<div class='container'>
  <div class='input-field'>
    <%= form_for :user, url: '/sessions' do |f| %>
      <%= f.label :email %><br>
      <%= f.text_field :email %><br>
  </div>

  <div class='input-field'>
      <%= f.label :password %><br>
      <%= f.text_field :password, type: :password %><br>

      <div class='waves-effect waves-light btn light-blue darken-3 right'>
        <%= f.submit 'login' %>
      </div>
    <% end %>
  </div>
</div>

<%= render '/errors' %>
<br><h4 class='container center-align'>login</h4><br>
<div class='container'>
    <%= form_for :user, url: '/sessions' do |f| %>
  <div class='input-field'>
      <%= f.label :email %><br>
      <%= f.text_field :email %><br>
  </div>

  <div class='input-field'>
      <%= f.label :password %><br>
      <%= f.text_field :password, type: :password %><br>
  </div>

  <button class='waves-effect waves-light btn blue-grey darken-1 right' type="submit" name="action">login
    <i class="material-icons right">keyboard_arrow_right</i>
  </button><br>
</div><br><br>
    <% end %>
<div class="container center">
  <span>Don't have an account? <%= link_to "register", new_user_path  %></span>
</div>
